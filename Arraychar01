package Array;

public class char1 
{
	public static void main(String[] args) {
		
		// array declaration
		
		char ch1[]=new char[4];
		
		// array initialization
		
		ch1[0]='a';
		ch1[1]='b';
		ch1[2]='c';
		ch1[3]='d';
		
		// usage
		for (int a=0;  a<=3;  a++)
		{
			System.out.println(ch1[a]);
		}
		
		
		
		
	}

}
